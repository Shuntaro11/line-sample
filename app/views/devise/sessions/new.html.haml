.account-page
  .center-box
    %p TIME
    %h1
      = link_to "新規登録", new_registration_path(resource_name), class: 'btn upbtn'
    = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      .field-input
        = f.email_field :email,autofocus: true, placeholder:"メールアドレス", class: 'form'
      .field-input
        = f.password_field :password, autocomplete: "off", placeholder:"パスワード", class: 'form'
      .actions
        = f.submit "ログイン", class: 'btn lowbtn'